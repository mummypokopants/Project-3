<script type="text/javascript">
  'use strict'
  $('form').submit(function (event) {
    event.preventDefault();
    var value = encodeURI($('input').val())
    var url = '/carparks/search?q=' + value
    $.get(url, function (data) {
      data.forEach(function (carpark) {
        console.log(carpark.address)
        console.log(carpark.weekday7amto1030pm)
        // $('tbody').append('tr').append('td').text(carpark.address)
        var row = $('<tr>')
        var address = $('<td>').text(carpark.address)
        var type = $('<td>').text(carpark.type)
        var weekdayMorn = $('<td>').text(carpark.weekday7amto1030pm)
        var weekdayEvening = $('<td>').text(carpark.weekday1030pmto7am)
        var saturdayMorn = $('<td>').text(carpark.Saturday7amto1030pm)
        var saturdayEvening = $('<td>').text(carpark.Saturday1030pmto7am)
        var sundayMorn = $('<td>').text(carpark.Sunday7amto1030pm)
        var sundayEvening = $('<td>').text(carpark.Sunday1030pmto7am)
        row.append(address)
        row.append(type)
        row.append(weekdayMorn)
        row.append(weekdayEvening)
        row.append(saturdayMorn)
        row.append(saturdayEvening)
        row.append(sundayMorn)
        row.append(sundayEvening)
        $('#search-results').append(row)
      })
    })
  })
</script>
